  /**
   * é€æ—¥æŸ¥è¯¢èŠ‚å‡æ—¥ï¼ˆæŒ‰æœˆæŸ¥è¯¢çš„ä¸»è¦å®ç°æ–¹å¼ï¼‰
   */
  private async getHolidaysByDateRangeFallback(startDate: Date, endDate: Date): Promise<Holiday[]> {
    const allHolidays: Holiday[] = [];
    const currentDate = new Date(startDate);
    let totalDays = 0;
    let holidayCount = 0;
    
    while (currentDate <= endDate) {
      totalDays++;
      const holidays = await this.getHolidaysByDate(new Date(currentDate));
      if (holidays.length > 0) {
        allHolidays.push(...holidays);
        holidayCount += holidays.length;
      }
      currentDate.setDate(currentDate.getDate() + 1);
      
      // æ·»åŠ å»¶è¿Ÿä»¥é¿å…APIè°ƒç”¨é¢‘ç‡è¿‡é«˜ï¼ˆæ¯10ä¸ªè¯·æ±‚å»¶è¿Ÿ100msï¼‰
      if (totalDays % 10 === 0) {
        await new Promise<void>((resolve: () => void) => {
          setTimeout(resolve, 100);
        });
      }
    }
    
    console.info(`é€æ—¥æŸ¥è¯¢å®Œæˆï¼šæŸ¥è¯¢${totalDays}å¤©ï¼Œæ‰¾åˆ°${holidayCount}ä¸ªèŠ‚å‡æ—¥`);
    return allHolidays;
  }

  /**
   * è·å–æŒ‡å®šæœˆä»½çš„èŠ‚å‡æ—¥
   * @param year å¹´ä»½
   * @param month æœˆä»½ï¼ˆ0-11ï¼‰
   * æ³¨æ„ï¼šç›´æ¥ä½¿ç”¨é€æ—¥æŸ¥è¯¢æ–¹å¼ï¼Œå› ä¸ºæŒ‰æœˆæŸ¥è¯¢APIå¯èƒ½è¿”å›æ ¼å¼ä¸å®Œæ•´
   */
  async getHolidaysByMonth(year: number, month: number): Promise<Holiday[]> {
    const startDate = new Date(year, month, 1);
    const endDate = new Date(year, month + 1, 0);
    console.info(`è·å–${year}å¹´${month + 1}æœˆçš„èŠ‚å‡æ—¥ï¼Œæ—¥æœŸèŒƒå›´: ${this.formatDateForAPI(startDate)} è‡³ ${this.formatDateForAPI(endDate)}`);
    return await this.getHolidaysByDateRangeFallback(startDate, endDate);
  }

  /**
   * ä»APIå“åº”è§£æèŠ‚å‡æ—¥
   */
  private parseHolidays(apiResponse: TianApiResponse, date: Date): Holiday[] {
    const holidays: Holiday[] = [];
    
    if (apiResponse.result) {
      const holiday = this.parseHolidayResult(apiResponse.result, date);
      if (holiday) {
        holidays.push(holiday);

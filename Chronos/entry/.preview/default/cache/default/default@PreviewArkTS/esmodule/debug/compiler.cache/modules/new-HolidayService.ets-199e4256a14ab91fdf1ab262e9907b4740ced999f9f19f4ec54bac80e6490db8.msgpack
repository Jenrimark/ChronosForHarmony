    // è·å–èŠ‚å‡æ—¥åç§°
    let name = result.name ?? '';
    // å¦‚æœnameä¸ºç©ºï¼Œæ ¹æ®ç±»å‹è®¾ç½®é»˜è®¤åç§°
    if (!name || name.length === 0) {
      if (daycode === 1) {

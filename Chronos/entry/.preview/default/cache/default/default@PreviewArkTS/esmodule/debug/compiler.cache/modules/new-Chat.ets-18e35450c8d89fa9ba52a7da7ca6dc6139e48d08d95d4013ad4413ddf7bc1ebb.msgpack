      .margin({ left: 12, right: 8 })

      // åŠ è½½åŠ¨ç”» - ç®€å•çš„ä¸‰ä¸ªç‚¹
      Row() {
        Text('â—')
          .fontSize(8)
          .fontColor(Constants.COLOR_TEXT_SECONDARY)
          .margin({ right: 3 })
        Text('â—')
          .fontSize(8)
          .fontColor(Constants.COLOR_TEXT_SECONDARY)
          .margin({ right: 3 })
        Text('â—')
          .fontSize(8)
          .fontColor(Constants.COLOR_TEXT_SECONDARY)
      }
      .backgroundColor(Constants.COLOR_CARD_BACKGROUND)

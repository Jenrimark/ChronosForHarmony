      } else if (daycode === 2) {
        name = 'å‘¨æœ«';
      } else if (daycode === 3) {
        name = result.info ?? 'è°ƒä¼‘';
      }
    }

    return new Holiday({
      name: name,
      date: dateStr,
      type: holidayType,
      daycode: daycode,
      isnotwork: isnotwork,

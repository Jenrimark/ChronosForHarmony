  }

  /**
   * æ ¼å¼åŒ–æ—¥æœŸä¸ºAPIéœ€è¦çš„æ ¼å¼ (YYYY-MM-DD)
   */
  private formatDateForAPI(date: Date): string {
    const year = date.getFullYear();
    const month = (date.getMonth() + 1).toString().padStart(2, '0');
